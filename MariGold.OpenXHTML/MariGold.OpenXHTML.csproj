<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Title>MariGold.OpenXHTML</Title>
    <Version>2.5.1</Version>
    <Authors>MariGold</Authors>
    <Description>A utility to convert html document to Open XML word document</Description>
    <Copyright>Copyright © MariGold 2015</Copyright>
    <PackageProjectUrl>https://github.com/kannan-ar/MariGold.OpenXHTML</PackageProjectUrl>
    <PackageTags>MariGold OpenXML HTML office</PackageTags>
    <Product>MariGold Framework</Product>
    <PackageReleaseNotes>Bug fix - image name with white spaces</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DocumentFormat.OpenXml" Version="2.8.1" />
    <PackageReference Include="MariGold.HtmlParser" Version="1.1.0" />
    <PackageReference Include="System.Drawing.Common" Version="4.5.0" />
    <PackageReference Include="System.Net.Http" Version="4.3.3" />
  </ItemGroup>

</Project>
